#include <iostream>
#include <string>
using namespace std;
bool isVowel(char ch) {
    int ascii= (int)ch;
    return (ascii == 65 || ascii == 69 || ascii == 73 || ascii == 79 || ascii == 85 ||   // A E I O U
            ascii == 97 || ascii == 101 || ascii == 105 || ascii == 111 || ascii == 117); // a e i o u
}
int main() {
    string str;
    cout << "Enter a string: ";
    getline(cin, str);
    string result = "";
    for(char ch : str){
        if(!isVowel(ch)){
            result += ch;  
        }
    }
    cout<<"String after deleting vowels: "<<result<< endl;
    return 0;
}
